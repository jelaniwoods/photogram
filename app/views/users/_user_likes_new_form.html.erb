<%= form_with(model: @like, local: true) do |f| %>

  <%= f.hidden_field :fan_id, value: @user.id %>

  <div class="form-group">
    <%= f.label :photo_id %>

    <%= f.text_field :photo_id, class: "form-control" %>
  </div>

  <%= f.submit "Create like", class: "btn btn-block btn-outline-secondary" %>
<% end %>
